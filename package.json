    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.7"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
