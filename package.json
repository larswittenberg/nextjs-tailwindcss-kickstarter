{
  "name": "nextjs-tailwindcss-kickstarter",
  "description": "Basic template setup with Next.js + Tailwind CSS and some helpers.",
  "version": "1.2.0",
  "homepage": "https://nextjs-tailwindcss-kickstarter.vercel.app/",
  "author": {
    "name": "Lars Wittenberg",
    "url": "https://larswittenberg.de"
  },
  "packageManager": "yarn@4.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larswittenberg/nextjs-tailwindcss-kickstarter.git"
  },
  "scripts": {
    "dev": "next dev",
    "dev-https": "next dev -p 3001 & npx local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "start": "next start",
    "clean": "npx rimraf .next",
    "cleanAll": "rm -rf node_modules && rm yarn.lock",
    "lint": "next lint",
    "format": "prettier --write .",
    "export": "npx rimraf .next && next build && next export"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.4.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "classnames": "^2.5.1",
    "negotiator": "^1.0.0",
    "next": "^15.4.7",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@next/eslint-plugin-next": "^15.4.1",
    "@types/mdx": "^2.0.13",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.89.2",
    "typescript": "^5.8.3"
  }
}
